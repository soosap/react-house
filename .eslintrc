{
  "ecmaFeatures": {
    "jsx": true
  },
  "env": {
    "browser": true,
    "node": true,
    "jquery": true
  },
  "rules": {
    "strict": [2, "global"],
    "quotes": [1, "single"],
    "no-trailing-spaces": 0,
    "eol-last": 1,
    "no-unused-vars": 0,
    "no-underscore-dangle": 1,
    "no-alert": 1,
    "no-lone-blocks": 1,
    "no-undef": 2,
    "semi": [1, "always"]
  },
  "globals": {
    "$": true,
    "jQuery": true
  }
}